<Window x:Class="wpf_structLayout.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:wpf_structLayout"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <TextBox x:Name="tb_src" HorizontalAlignment="Left" Margin="76,12,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="642" Height="22" DataContextChanged="tb_src_DataContextChanged" AcceptsReturn="True"/>
        <Label x:Name="label" Content="PDB File" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top"/>
        <Label x:Name="label_Copy" Content="Detected Structs" HorizontalAlignment="Left" Margin="10,55,0,0" VerticalAlignment="Top"/>
        <DataGrid x:Name="dg_output" d:ItemsSource="{d:SampleData ItemCount=5}" Margin="405,89,0,0" Loaded="dg_output_Loaded"/>
        <Button x:Name="bt_analyze" Content="Analyze" HorizontalAlignment="Left" Margin="734,13,0,0" VerticalAlignment="Top" Width="56"/>

    </Grid>
</Window>
